  </servlet>
  <servlet-mapping>
    <servlet-name>ThemLoaiHoaController</servlet-name>
    <url-pattern>/them-loai-hoa</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>XoaHoaController</servlet-name>
    <servlet-class>controller.XoaHoaController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>XoaHoaController</servlet-name>
    <url-pattern>/xoa-hoa</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SuaHoaController</servlet-name>
    <servlet-class>controller.SuaHoaController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SuaHoaController</servlet-name>
    <url-pattern>/sua-hoa</url-pattern>
  </servlet-mapping>
</web-app>